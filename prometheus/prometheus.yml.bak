global:
  scrape_interval: 30s # Set the scrape interval to every 30 seconds.
  evaluation_interval: 30s # Evaluate rules every 30 seconds.

scrape_configs:
  - job_name: "Avail"
    static_configs:
      - targets: [ "HOST_IP:10616" ]  
  - job_name: "Node"
    static_configs:
      - targets: [ "node-exporter:9100" ]
